{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts spec/**/*.ts",
    "test": "cross-env NODE_ENV=test db-migrate --env test up && cross-env NODE_ENV=test ts-node --transpile-only ./node_modules/jasmine/bin/jasmine.js --config=./spec/support/jasmine.json && npm run db:down -- --env test -c 100",
    "format": "prettier --write \"src/**/*.ts\" \"spec/**/*.ts\"",
    "db:create": "npx db-migrate create -t ts",
    "db:up": "npx db-migrate up",
    "db:down": "npx db-migrate down"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.5.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.10",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.2.9",
    "typescript-eslint": "^8.46.2"
  }
}
